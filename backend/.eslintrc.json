{
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2015,
      "sourceType": "module"
    },
    "parser": "babel-eslint",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": ["error", {
       "endOfLine":"auto"
     }]
    }
  }